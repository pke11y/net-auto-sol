version 12.4
service timestamps debug datetime msec
service timestamps log datetime msec
no service password-encryption
!
hostname {{ item }}
!
boot-start-marker
boot-end-marker
!
logging buffered 4096 informational
enable password pk
!         
no aaa new-model
no network-clock-participate slot 1 
no network-clock-participate wic 0 
ip cef
!
!
!
!
no ip domain lookup
ip domain name pk.lab
ip auth-proxy max-nodata-conns 3
ip admission max-nodata-conns 3
!
ipv6 unicast-routing
!         
!
!
!
!
!
!
!
!
!
!
!
!
!
username pk privilege 15 password 0 pk
!         
!
ip ssh version 2
! 
!
!
!
!
interface Loopback0
 ip address 10.10.10.11 255.255.255.255
!
interface FastEthernet0/0
 no ip address
 duplex auto
 speed auto
!         
interface FastEthernet0/0.200
 encapsulation dot1Q 200
 ip address 10.0.0.56 255.255.255.0
!
interface FastEthernet0/0.1031
 encapsulation dot1Q 1031
 ip address 10.0.3.1 255.255.255.252
!
interface Serial0/0
 no ip address
 encapsulation frame-relay
!
interface Serial0/0.12 point-to-point
 ip address 10.1.1.1 255.255.255.252
 ip nat inside
 ip virtual-reassembly
 ip ospf authentication message-digest
 ip ospf message-digest-key 1 md5 TSHOOT
 ip ospf network point-to-point
 ipv6 address 2026::12:1/122
 ipv6 ospf 1 area 12
 frame-relay interface-dlci 102   
!
interface Serial0/1
 ip address 209.65.200.225 255.255.255.252
 ip nat outside
 ip virtual-reassembly
 encapsulation frame-relay
 clock rate 128000
 frame-relay interface-dlci 81
 frame-relay intf-type dce
!
router ospf 1
 router-id 10.1.1.1
 log-adjacency-changes
 network 10.1.1.0 0.0.0.3 area 12
 network 172.31.100.0 0.0.0.255 area 12
 default-information originate
!
router bgp 65501
 bgp router-id 10.0.0.56
 bgp log-neighbor-changes
 neighbor 10.0.3.2 remote-as 65001
 neighbor 10.0.3.2 description PE1
 !        
 address-family ipv4
  neighbor 10.0.3.2 activate
  no auto-summary
  no synchronization
 exit-address-family
!
ip forward-protocol nd
ip route 0.0.0.0 0.0.0.0 10.0.0.253
ip route 10.0.0.0 255.0.0.0 Null0
!
!
ip http server
no ip http secure-server
ip nat inside source list NAT-POOL interface Serial0/1 overload
!         
ip access-list standard NAT-POOL
 permit 10.1.0.0 0.0.255.255
 permit 10.2.0.0 0.0.255.255
!
ipv6 router ospf 1
 router-id 10.1.1.1
 log-adjacency-changes
!
!
route-map BGP_OUT permit 10
!
!
!
control-plane
!         
!
!
!
!
!
!
!
!
!
line con 0
 exec-timeout 0 0
line aux 0
line vty 0 4
 login local
!         
!
end